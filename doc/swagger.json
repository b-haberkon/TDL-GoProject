{
    "openapi": "3.0.3",
    "info": {
      "version": "1.0.0",
      "title": "API Koi",
      "license": {
        "name": "PROPIETARY"
      }
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000/api"
        }
    ],
    "paths": {
        "/register": {
            "post" : {
                "operationId": "controllers_Register",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            },
        "/login": {
            "post" : {
                "operationId":"controllers_Login",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            },
        "/user": {
            "get" : {
                "operationId":"controllers_User",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            },
        "/logout": {
            "post" : {
                "operationId":"controllers_Logout",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            },
        "/memo": {
            "post" : {
                "operationId":"memotest_CreateGame",
                "summary":"Create and join a game",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            },
        "/memo/{gameId}": {
            "get" : {
                "operationId":"memotest_ShowGame",
                "summary":"Watch an existing game",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                },
            "post" : {
                "operationId":"memotest_JoinGame",
                "summary":"Join an existing game",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                },
            "parameters": [
                {
                    "name":"gameId",
                    "in":"path",
                    "description": "Id of the game, given in memotestCreateGame",
                    "required":true,
                    "schema": {
                        "type":"number"
                    }
                }
            ]
            },
        "/memo/{gameId}/{piece}": {
            "put" : {
                "operationId":"memotest_SelectPiece",
                "summary":"Select a piece",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                },
            "delete" : {
                "operationId":"memotest_DeselectPiece",
                "summary":"Deselect a piece",
                "responses": {
                    "200": {
                        "description": "Ok"
                        }
                    }
                }
            }
    
    }
}